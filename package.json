{
  "name": "@nozzlegear/react-svg-progress",
  "version": "1.3.1",
  "description": "An indefinite circular progress inspired by Google's progress indicator. Based off of https://github.com/nathanhoad/react-loading-animation.",
  "main": "dist/index.js",
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p . && vite build tests",
    "validate": "tsc -p . --noEmit",
    "prepublishOnly": "npm run build",
    "watch": "vite build tests && vite --open --base /tests/dist tests/dist",
    "test": "echo 'Run `npm run watch` to test this package in the browser.'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nozzlegear/react-svg-progress.git"
  },
  "keywords": [
    "react",
    "svg",
    "progress",
    "circular progress",
    "indicator"
  ],
  "author": "Joshua Harms <joshua@nozzlegear.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nozzlegear/react-svg-progress/issues"
  },
  "homepage": "https://github.com/nozzlegear/react-svg-progress#readme",
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3",
    "utilities.styl": "^1.8.1",
    "vite": "^6.0.11"
  }
}
